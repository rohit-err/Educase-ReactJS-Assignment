{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(js|css|json|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|map)",
      "dest": "/$1.$2"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}

